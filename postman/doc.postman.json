			"script": {
				"type": "text/javascript",
				"exec": [
					"const bootstrap = async () => {",
					"    const api = pm.request.url.path.join('/')",
					"    if (api !== \"api/auth/login\") return",
					"    const { accessToken } = await pm.response.json()",
					"    pm.collectionVariables.set(\"accessToken\", accessToken)",
					"}",
					"bootstrap()"
				]
			}
		}
